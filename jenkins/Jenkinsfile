pipeline {
	agent any
    stages {
		stage('Checkout') {
			steps {
				git 'https://github.com/PetFactory1991/JavaSelenideJenkinsAllureDockerSelenoid.git'
            }
        }
        stage('Run Tests') {
			steps {
				sh 'mvn clean test -Dremote=http://165.22.93.142:4444/wd/hub'
            }
        }
    }
}
